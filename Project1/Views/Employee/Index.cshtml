@model List<Project1.Models.dbo_employee>
@{
    ViewBag.Title = "Index";
    int sn = 0;
}
<a href="@Url.Action("Create")">Add new record</a>
<style>
    .table {
        margin: 1px;
        padding: 5px;
        background-color: antiquewhite;
    }
</style>
<h2>Employee list</h2>
<table class="table" border="1">
    <tr>
        <th>SN</th>
        <th>id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Number</th>
        <th>Edit</th>
    </tr>
    @foreach (var data in Model)
    {
<tr>
    <td>@(++sn) </td>
    <td>@(data.id) </td>
    <td>@(data.FirstName) </td>
    <td>@(data.LastName) </td>
    <td>@(data.Phone) </td>


    <td><a href="@Url.Action("Edit", new {id=data.id})">Edit</a></td>
</tr>
    }
</table>

    